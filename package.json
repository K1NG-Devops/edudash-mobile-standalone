{
  "name": "edudash-pro",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:fresh": "node clear-cache.js && expo start",
    "clear-cache": "node clear-cache.js",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "check-rules": "node scripts/check-rules.js",
    "validate-rules": "node scripts/validate-project-rules.js",
    "test:rules": "npm run type-check && npm run check-rules && npm run lint",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "build:production": "eas build --platform android --profile production",
    "build:local": "./build-local.sh",
    "android:local": "ANDROID_HOME=/home/king/Android/Sdk npx expo run:android --device",
    "prebuild:android": "npx expo prebuild --platform android --clear",
    "dev": "./start-development.sh",
    "dev:tunnel": "npx expo start --tunnel --clear",
    "build:dev": "npx eas build --platform android --profile development --local",
    "preview": "eas build --platform android --profile preview",
    "test:invite": "node scripts/smoke-test-teacher-invite.js",
    "test:invite:parent": "node scripts/smoke-test-parent-invite.js",
    "func:serve": "make func-serve FN=$FN ENV=$ENV",
    "func:invoke": "make func-invoke FN=$FN DATA=$DATA ENV=$ENV",
    "func:deploy": "make func-deploy FN=$FN PROJECT_REF=$PROJECT_REF",
    "func:secrets": "make secrets PROJECT_REF=$PROJECT_REF SERVER_SUPABASE_SERVICE_ROLE_KEY=$SERVER_SUPABASE_SERVICE_ROLE_KEY SERVER_RESEND_API_KEY=$SERVER_RESEND_API_KEY SERVER_NOTIFY_FUNCTION_TOKEN=$SERVER_NOTIFY_FUNCTION_TOKEN FROM_EMAIL=$FROM_EMAIL",
    "func:secrets-legacy": "make secrets-legacy PROJECT_REF=$PROJECT_REF SUPABASE_SERVICE_ROLE_KEY=$SUPABASE_SERVICE_ROLE_KEY RESEND_API_KEY=$RESEND_API_KEY NOTIFY_FUNCTION_TOKEN=$NOTIFY_FUNCTION_TOKEN",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:principal": "jest __tests__/principal-dashboard.test.ts",
    "test:components": "jest __tests__/components/",
    "test:integration": "jest __tests__/integration/",
    "test:all": "npm run test:principal && npm run test:components && npm run test:integration"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.58.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@supabase/supabase-js": "^2.55.0",
    "@types/base64-arraybuffer": "^0.2.4",
    "ajv": "^8.0.0",
    "base64-arraybuffer": "^1.0.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "expo": "53.0.22",
    "expo-blur": "~14.1.5",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "^5.2.4",
    "expo-document-picker": "~13.1.6",
    "expo-file-system": "^18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.5",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-google-mobile-ads": "^14.9.0",
    "react-native-heroicons": "^4.0.0",
    "react-native-qrcode-svg": "^6.3.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.3",
    "@testing-library/react-native": "^12.8.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eas-cli": "^16.17.4",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.10",
    "react-test-renderer": "^19.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "~5.8.3"
  },
  "resolutions": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "overrides": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "private": true
}
