{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "cache": {
        "disabled": true
      }
    },
    "preview": {
      "distribution": "internal",
      "cache": {
        "disabled": true
      }
    },
    "production": {
      "autoIncrement": true,
      "cache": {
        "disabled": false
      },
      "env": {
        "NODE_ENV": "production"
      },
      "node": "20.x.x"
    }
  },
  "submit": {
    "production": {}
  }
}
